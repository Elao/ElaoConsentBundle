    "keywords": ["elao", "symfony", "consent", "rgpd", "cookies"],
    "homepage": "https://github.com/Elao/ElaoConsentBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "https://www.elao.com"
        },
        {
            "name": "Maxime COLIN",
            "email": "maxime.colin@elao.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "symfony/framework-bundle": "~5.0",
        "symfony/twig-bundle": "~5.0",
        "symfony/asset": "~5.0",
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
